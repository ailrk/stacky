{
  "manifest_version": 3,
  "name": "Stacky",
  "version": "1.0",
  "description": "Records visited pages and organizes them in stacks.",
  "permissions": ["history", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}

